<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:label="@string/title_map" />

    <fragment
        android:id="@+id/navigation_people"
        android:name="com.android2.ui.people.PeopleFragment"
        android:label="@string/title_people"
        tools:layout="@layout/fragment_people" />

    <fragment
        android:id="@+id/navigation_compass"
        android:name="com.android2.ui.compass.CompassFragment"
        android:label="@string/title_compass"
        tools:layout="@layout/fragment_compass" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.android2.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

</navigation>
